@charset "utf-8";
.logo{float:left;margin-left:9px;margin-top:15px;}
.lnb{float:left;margin-left:238px;*zoom:1;}
.lnb:after{content:"";display:block;clear:both;}
.lnb li{float:left;height:68px;border-right:1px solid #f0f0f0;}
.lnb li:first-child{}
.lnb .search{width:349px;position:relative;}
.search_img{position:absolute;top:0;right:0;width:70px;height:68px;background:url(../img/gnb_li_bg.png) no-repeat center center;text-indent:-9999px;cursor:pointer;}
.search_img:hover{background:url(../img/gnb_li_on.png) no-repeat center center;}
.search_box{position:absolute;top:23px;left:15px;*zoom:1;}
.search_box:after{content:"";display:block;clear:both;}
.search_box input{float:left;}
.search_input{padding-left:5px;padding-right:5px;border:1px solid #ddd;width:174px;height:23px;line-height:23px;background:#fff;}
.search_btn{border:none;background:url(../img/search_btn.png) no-repeat;width:74px;height:25px;cursor:pointer;}
.search_on{background:url(../img/gnb_li_on.png) no-repeat center center !important;}

.lnb .all_menu{width:73px;background:url(../img/toggle_btn.png) no-repeat center center;}
.lnb .all_menu:hover{background:url(../img/toggle_btn_on.png) no-repeat center center;}
.all_on{background:url(../img/toggle_btn_on.png) no-repeat center center !important;}
.lnb .tsubaki{width:125px;}




.gnb_wrap{position:relative;}
.gnb{*zoom:1;border-right:1px solid #eaeaea;}
.gnb:after{content:"";display:block;clear:both;}
.gnb > li{float:left;padding-top:20px;width:274px;height:47px;border-left:1px solid #eaeaea;border-bottom:1px solid #eaeaea;text-align:center;cursor:pointer;}
.gnb > li:first-child{width:273px;}
.gnb > li > a{display:block;height:48px;}
.gnb > li > img{}


.snb_wrap{*zoom:1;}
.snb_wrap:after{content:"";display:block;clear:both;}
.snb{border-right:1px solid #eaeaea;*zoom:1;}
.snb:after{content:"";display:block;clear:both;}
.snb li{float:left;padding-top:11px;padding-left:15px;width:141px;height:38px;border-left:1px solid #eaeaea;border-bottom:1px solid #eaeaea;text-align:left;}
.snb li img{}
.snb_right{float:right;}
.snb_right li{text-align:center;padding-top:13px;height:36px;}
.snb_right li a{padding:5px;display:block;}
.snb_right li img{}
.snb li:hover{background:#f5f5f5;}


/* 배너 슬라이드 */
.eco_wrap{position:relative;width:1098px;margin:0 auto;border-left:1px solid #ddd;border-right:1px solid #ddd;border-bottom:1px solid #ddd;}
.banner_slider{position:relative;width:1098px;height:600px;margin:0 auto;}
.banner_container{position:relative;overflow:hidden;width:1098px;height:600px;}
#banner_content{position:absolute;width:3294px;text-align:center;}
#banner_content img{float:left;}
.banner_slider .banner_nav_container{position:absolute;vertical-align:middle;right:0px;top:560px;width:100px;}
#banner_nav{position:absolute;list-style:none;}
#banner_nav li{float:left;margin-right:3px;width:20px;height:20px;}
#banner_nav li a{float:left;width:20px;height:20px;line-height:20px;background:url(../img/banner_nav_dot.png) no-repeat;	}
#banner_nav li a.select{background:url(../img/banner_nav_dot_on.png) no-repeat;	width:20px;height:20px;}
.banner_close{position:absolute;top:10px;right:20px;cursor:pointer;}



/* index */
.main_wrap{margin:0 auto;width:1099px;border-left:1px solid #ddd;}
.main_wrap:after{content:"";display:block;clear:both;}
.board_box{float:left;padding-top:30px;padding-left:24px;width:525px;height:219px;border-right:1px solid #ddd;border-bottom:1px solid #ddd;}
.board_box h3 span{margin-left:10px;}
.board_list{margin-top:20px;}
.board_list li{line-height:26px;background:url(../img/free_module_dot.jpg) no-repeat left center;}
.board_list li a{padding-left:10px;color:#000;}
.board_list li:hover a{color:#00a0f7;}



.in_menu_list{float:left;}
.in_menu_list:after{content:"";display:block;clear:both;}
.in_menu_list > li{position:relative;float:left;width:274px;height:249px;border-bottom:1px solid #ddd;border-right:1px solid #ddd;}
.in_menu_list > li.small_list{width:273px;}
.in_menu_list > li.menu_1{background:url(../img/menu_1.png) no-repeat;}
.in_menu_list > li.menu_2{background:url(../img/menu_2.png) no-repeat;}
.in_menu_list > li div.list_div{position:absolute;left:0;top:0px;width:100%;height:209px;background:#000;text-align:center;padding-top:40px;}

.list_div h3{padding-left:25px;margin-left:15px;margin-bottom:15px;color:#fff;font-size:14px;text-align:left;background:url(../img/op_h3_bg.png) no-repeat left center;line-height:20px;}
.op_menu_list{margin-left:15px;}
.op_menu_list li{margin-left:20px;text-align:left;color:#fff;background:url(../img/op_bg.jpg) no-repeat left center;}
.op_menu_list li a{padding-left:6px;color:#fff;line-height:20px;font-size:12px;}
.op_menu_list li:hover a{color:#00a0f7;}
.clear_box{*zoom:1;}
.clear_box:after{content:"";display:block;clear:both;}



.in_menu_list2:after{content:"";display:block;clear:both;}
.in_menu_list2 > li{position:relative;float:left;width:274px;height:249px;border-bottom:1px solid #ddd;border-right:1px solid #ddd;}
.in_menu_list2 > li.small_list{width:273px;}
.in_menu_list2 > li.bo_no{border-bottom:none;}
.in_menu_list2 > li.menu_3{background:url(../img/menu_3.png) no-repeat;}
.in_menu_list2 > li.menu_4{background:url(../img/menu_4.png) no-repeat;}
.in_menu_list2 > li.menu_5{background:url(../img/menu_5.png) no-repeat;}
.in_menu_list2 > li.menu_6{background:url(../img/menu_6.png) no-repeat;}
.in_menu_list2 > li.menu_7{background:url(../img/menu_7.png) no-repeat;}
.in_menu_list2 > li.menu_8{background:url(../img/menu_8.png) no-repeat;}
.in_menu_list2 > li.menu_9{background:url(../img/menu_9.png) no-repeat;}
.in_menu_list2 > li.menu_10{background:url(../img/menu_10.png) no-repeat;}
.in_menu_list2 > li.menu_11{background:url(../img/menu_11.png) no-repeat;}
.in_menu_list2 > li.menu_12{background:url(../img/menu_12.png) no-repeat;}
.in_menu_list2 > li.menu_13{background:url(../img/menu_13.png) no-repeat;}
.in_menu_list2 > li.menu_14{background:url(../img/menu_14.png) no-repeat;}
.in_menu_list2 > li div.list_div{position:absolute;left:0;top:0;width:100%;height:209px;background:#000;text-align:center;padding-top:40px;}


.p_box{position:absolute;top:210px;width:100%;text-align:center;}



/* 서브 배너 */
.sub_banner{border-bottom:1px solid #ebebeb;}
.sub_banner > img{width:100%;height:160px;}

/* lnb */
.sub_contentns{*zoom:1;}
.sub_contents:after{content:"";display:block;clear:both;}
.sub_contents > div{float:left;}


.info_box{border-bottom:1px solid #eaeaea;padding:32px 35px 36px 33px;}
.info_box p{margin-top:10px;}

.pro_list{}
.pro_list li{padding:20px 33px;border-bottom:1px solid #eaeaea;*zoom:1;}
.pro_list li:after{content:"";display:block;clear:both;}
.pro_list li img{margin-right:13px;border:1px solid #eaeaea;}
.pro_list li img, .pro_list div{float:left;}
.pro_list h5{color:#435e7a;}
.pro_list p.info_p{margin-top:18px;color:#666;}
.pro_list div{position:relative;width:675px;height:100px;*zoom:1;}
.pro_list div:after{content:"";display:block;clear:both;}
.pro_list p.line_up{position:absolute;right:0;bottom:0;}
.pro_list p.line_up a{color:#00a0f7;text-decoration:underline;}
.pro_list li:hover{background:#fafafa;}
.pro_list .line_p{position:absolute;left:0; bottom:0;}
.ebook{float:none !important;margin-left:10px;margin-right:0px !important;vertical-align:bottom;}

/* sub_page_2 */

.view_info_box{padding:20px 33px;border-bottom:1px solid #eaeaea;}
.view_info_box:after{content:"";display:block;clear:both;}
.view_left{float:left;}
.view_left img{margin-right:15px;border:1px solid #eaeaea;}
.view_right{float:left;width:600px;}
.view_right h5{font-size:14px;color:#000;}
.pro_info{margin-top:20px;}
.pro_info dt{color:#566d85;line-height:21px;}
.pro_info dd{color:#666;line-height:21px;}
.eco_go{margin-top:10px;}
.data_wrap{padding:49px 0;border-bottom:1px solid #eaeaea;}
.data_table{margin:0 auto;width:452px;border-top:1px solid #eaeaea;border-right:1px solid #eaeaea;text-align:center;}
.data_table td{height:35px;border-left:1px solid #eaeaea;border-bottom:1px solid #eaeaea;}

/* sub_page_3 */
.view_clear:after{content:"";display:block;clear:both;}
.btn_3{margin-top:40px;}